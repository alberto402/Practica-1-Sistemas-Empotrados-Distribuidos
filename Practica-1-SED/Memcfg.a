#Bank 7(if SROM) parameter
.equ 	B7_Tacs		,0x3    /* 4clk              */                     
.equ 	B7_Tcos		,0x3    /* 4clk              */                     
.equ 	B7_Tacc		,0x7    /* 14clk             */                     
